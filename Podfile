use_frameworks!
  
target 'pubnudge' do
    # Can only support iOS 8 or higher with frameworks, 
    # but can only target higher frameworks as well
    platform :ios, '8.0' # (or '9.0' or '10.0')
    pod "PubNub", "~> 4"
end